#include "adc.h"
#include "tim.h"
#include "usart.h"
#include "gpio.h"
#include "string.h"

void stopMotor() {
    // Desactivez l'Ã©tage de puissance du moteur (code pour desactiver le moteur)

    // Affichez le message "Power OFF" dans la console
    HAL_UART_Transmit(&huart2, "Power OFF\r\n", strlen("Power OFF\r\n"), HAL_MAX_DELAY);
}
